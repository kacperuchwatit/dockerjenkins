pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('https://github.com/kacperuchwatit/dockerjenkins.git') { 
            steps { 
                script{
                checkout scm
                }
            }
        }
        stage('Pull from ECR') {
            steps {
                sh "sudo docker pull 943696080604.dkr.ecr.us-west-1.amazonaws.com/k8s:latest"
            }
        }
        stage('Deploy to EKS') {
            steps {
                sh "kubectl set image deployment/aws-pod your-container-name=943696080604.dkr.ecr.us-west-1.amazonaws.com/k8s:latest -n aws-app"
            }
        }
    }
}
